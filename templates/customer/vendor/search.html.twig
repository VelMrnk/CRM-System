{%  for key, vendor in vendors %}
    <tr role="row" class="odd {% if key is not divisible by (2) %} row-style {% endif %}">
        <td><a href="{{ path('vendor_edit', {'id': vendor.id}) }}">{{ vendor.name | upper }}</a></td>
        <td>
            {% if vendor.isActive %}
                <label class="label label-info">{{ 'table.active'|trans({}, 'labels') }}</label>
            {% else %}
                <label class="label label-warning">{{ 'table.inactive'|trans({}, 'labels') }}</label>
            {% endif %}
        </td>
    </tr>
{% endfor %}